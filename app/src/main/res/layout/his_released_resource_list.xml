<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">
    <biz.home.api.myComponent.Topbar
        android:id="@+id/his_released_resource_topbar"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:title2="他发布的资源"></biz.home.api.myComponent.Topbar>
    <ListView
        android:layout_margin="5dp"
        android:id="@+id/his_released_resource_listview"
        android:layout_below="@+id/his_released_resource_topbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </ListView>
    <Button
        android:id="@+id/his_released_resource_button_call"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@color/speakToit_bg"
        android:textColor="@color/white"
        android:text="打他电话"/>
</RelativeLayout>